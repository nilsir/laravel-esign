{
    "name": "nilsir\/laravel-esign",
    "description": "esign for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "nilsir",
            "email": "nilsir@qq.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.5",
        "doctrine/cache": "^1.10",
        "monolog/monolog": "^2.0",
        "pimple/pimple": "^3.3",
        "symfony/http-foundation": "^5.0",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Nilsir\\LaravelEsign\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "mockery/mockery": "^1.3"
    },
    "extra": {
